{
    "name": "laravel-notification-channels/mobitel-sms",
    "description": "Laravel Notification Channel for [Mobitel](https://mobitel.lk) SMS",
    "homepage": "https://github.com/laravel-notification-channels/mobitel-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Saranga Abeykoon",
            "email": "amisaranga@gmail.com",
            "homepage": "http://nterms.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\MobitelSMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\MobitelSMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
